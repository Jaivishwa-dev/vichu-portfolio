<script>
   // Function to show "Thank You" message after form submission
   function showThankYouMessage(event) {
      event.preventDefault(); // Prevent the form from submitting normally
      document.getElementById("contact-form").style.display = "none"; // Hide the form
      document.getElementById("thank-you-message").style.display = "block"; // Show thank you message
   }
</script>

<script>
  // Add hover effect with JavaScript to handle the glow transition
  document.querySelectorAll('.nav-big').forEach(item => {
    item.addEventListener('mouseenter', function() {
      // Apply glowing effect to the text
      this.style.transition = 'color 0.3s, text-shadow 0.5s ease';
      this.style.textShadow = '0 0 8px rgba(128, 0, 128, 1), 0 0 25px rgba(128, 0, 128, 1)';
      this.style.color = '#800080';  // Apply purple color to the text as well
    });

    item.addEventListener('mouseleave', function() {
      // Reset text glow effect
      this.style.textShadow = 'none';
      this.style.color = 'inherit';  // Reset the text color
    });
  });
</script>

       </div>
    </div>

