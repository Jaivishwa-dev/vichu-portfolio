<style>
   /* Form Container */
   .form-container {
      max-width: 600px;
      margin: 40px auto;
      padding: 50px;
      background: linear-gradient(135deg, #c0c0c0, #f0f0f0);
      border-radius: 16px;
      box-shadow: 0 8px 24px rgba(0, 0, 0, 0.1);
      color: #333;
      font-family: 'Arial', sans-serif;
      transition: transform 0.2s ease-in-out;
   }

   .form-container:hover {
      transform: scale(1.02); /* Subtle hover effect */
   }

   .form-group {
      position: relative;
      margin-bottom: 30px;
   }

   .form-control {
      width: 100%;
      padding: 18px 20px;
      font-size: 18px;
      border: 2px solid #ccc;
      border-radius: 12px;
      background-color: rgba(255, 255, 255, 0.8); /* Semi-transparent white */
      color: #333;
      outline: none;
      transition: all 0.3s ease;
   }

   .form-control:focus {
      background-color: rgba(255, 255, 255, 1);
      border-color: transparent; /* Change border color to transparent */
      box-shadow: 0 0 10px rgba(76, 145, 216, 0.5);
   }

   /* Floating label */
   .floating-label {
      position: absolute;
      top: 16px;
      left: 20px;
      font-size: 16px;
      color: rgba(51, 51, 51, 0.7);
      pointer-events: none;
      transition: all 0.3s ease;
   }

   .form-control:focus ~ .floating-label,
   .form-control:not(:placeholder-shown) ~ .floating-label {
      top: -10px;
      left: 15px;
      font-size: 14px;
      color: #4b0082;
   }

   /* Placeholder color change */
   .form-control::placeholder {
      color: #888; /* Light gray color for placeholder */
      opacity: 1; /* Ensures the placeholder color is applied correctly */
   }

   .form-control:focus::placeholder {
      color: #4c91d8; /* Change placeholder color to blue when focused */
   }

   .btn-submit {
      width: 100%;
      padding: 16px;
      font-size: 20px;
      font-weight: bold;
      color: white;
      background: linear-gradient(135deg, #4c91d8, #38b7ff);
      border: none;
      border-radius: 12px;
      cursor: pointer;
      box-shadow: 0 6px 20px rgba(0, 0, 0, 0.1);
      transition: background 0.3s ease, transform 0.2s ease;
   }

   .btn-submit:hover {
      background: linear-gradient(135deg, #38b7ff, #4c91d8);
      transform: translateY(-4px);
   }
</style>

<script>
   // JavaScript to detect if any input field is focused
   const formContainer = document.querySelector('.form-container');
   const formControls = document.querySelectorAll('.form-control');

   formControls.forEach(input => {
      input.addEventListener('focus', () => {
         formContainer.classList.add('focused'); // Add class when any input is focused
      });

      input.addEventListener('blur', () => {
         formContainer.classList.remove('focused'); // Remove class when input loses focus
      });
   });
</script>



</form>

