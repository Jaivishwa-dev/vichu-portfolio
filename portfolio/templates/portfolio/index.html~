      <h2>Thank You!</h2>
      <p>Your message has been successfully sent. I will get back to you soon.</p>
      <button onclick="closePopup()" class="close-btn">Close</button>
   </div>
</div>

<script>
   function submitForm(event) {
      event.preventDefault(); // Prevent form from submitting and page reload
      const form = document.getElementById('contact-form');
      const formData = new FormData(form);

      fetch(window.location.href, {
         method: 'POST',
         body: formData,
      })
      .then(response => response.json())
      .then(data => {
         if (data.message) {
            document.getElementById('thank-you-popup').style.display = 'flex'; // Show the pop-up message
            form.reset(); // Optionally reset the form fields
         }
      })
      .catch(error => {
         console.error('Error:', error);
      });
   }

   function closePopup() {
      document.getElementById('thank-you-popup').style.display = 'none'; // Hide the pop-up when 'Close' is clicked
   }
</script>

